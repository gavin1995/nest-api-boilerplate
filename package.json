{
  "name": "nest-api-boilerplate",
  "version": "1.0.0",
  "description": "nestjs8+ rest api boilerplate",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:debug": "npm run postbuild && nest start --debug --watch",
    "start:dev": "cross-env APP_NAME=nest-api-boilerplate npm run postbuild && nest start --watch",
    "start:test": "cross-env APP_NAME=nest-api-boilerplate RUN_ENV=test npm run postbuild && cross-env APP_NAME=nest-api-boilerplate RUN_ENV=test nest start",
    "start:pre": "cross-env APP_NAME=nest-api-boilerplate RUN_ENV=pre npm run postbuild && cross-env APP_NAME=nest-api-boilerplate RUN_ENV=pre node dist/main",
    "start:prod": "cross-env APP_NAME=nest-api-boilerplate RUN_ENV=prod npm run postbuild && cross-env APP_NAME=nest-api-boilerplate RUN_ENV=prod node dist/main",
    "postbuild": "cpy 'src/**/*.json' 'dist/src'",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nest",
    "nestjs",
    "nestjs8",
    "rest",
    "api",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gavin1995/nest-api-boilerplate.git"
  },
  "author": "Gavin<gavin199502@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gavin1995/nest-api-boilerplate/issues"
  },
  "homepage": "https://github.com/gavin1995/nest-api-boilerplate#readme",
  "dependencies": {
    "@nestjs/common": "^10.3.9",
    "@nestjs/core": "^10.3.9",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/sequelize": "^10.0.1",
    "@nestjs/swagger": "^7.3.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.10",
    "helmet": "^7.1.0",
    "lodash": "^4.17.21",
    "mint-filter": "^4.0.3",
    "mysql2": "^3.10.1",
    "node-fetch": "^3.3.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.7",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2"
  }
}
