{
  "name": "nest-api-boilerplate",
  "version": "1.0.0",
  "description": "nestjs8+ rest api boilerplate",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:debug": "npm run postbuild && nest start --debug --watch",
    "start:dev": "cross-env APP_NAME=nest-api-boilerplate npm run postbuild && nest start --watch",
    "start:test": "cross-env APP_NAME=nest-api-boilerplate RUN_ENV=test npm run postbuild && cross-env APP_NAME=nest-api-boilerplate RUN_ENV=test nest start",
    "start:pre": "cross-env APP_NAME=nest-api-boilerplate RUN_ENV=pre npm run postbuild && cross-env APP_NAME=nest-api-boilerplate RUN_ENV=pre node dist/main",
    "start:prod": "cross-env APP_NAME=nest-api-boilerplate RUN_ENV=prod npm run postbuild && cross-env APP_NAME=nest-api-boilerplate RUN_ENV=prod node dist/main",
    "postbuild": "cpy --cwd=src --parents '**/*.json' ../dist/src",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nest",
    "nestjs",
    "nestjs8",
    "rest",
    "api",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gavin1995/nest-api-boilerplate.git"
  },
  "author": "Gavin<gavin199502@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gavin1995/nest-api-boilerplate/issues"
  },
  "homepage": "https://github.com/gavin1995/nest-api-boilerplate#readme",
  "dependencies": {
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/swagger": "^5.0.9",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "ejs": "^3.1.6",
    "helmet": "^4.6.0",
    "lodash": "^4.17.21",
    "mint-filter": "^3.0.1",
    "mysql2": "^2.3.0",
    "nestjs-redis": "git+https://github.com/GyanendroKh/nestjs-redis.git#nest8-fix",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "sequelize": "^6.6.5",
    "sequelize-typescript": "^2.1.0",
    "source-map-support": "^0.5.19",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.6",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "cpy-cli": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.5",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
